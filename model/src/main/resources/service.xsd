<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://www.shopzilla.com/services/catalog"
  targetNamespace="http://www.shopzilla.com/services/catalog" elementFormDefault="qualified"
  attributeFormDefault="unqualified" version="1.0">

    <xs:annotation>
        <xs:documentation>
    <!--
